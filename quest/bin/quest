#!/bin/bash

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )"

case $1 in
new)
  $DIR/new "${@:2}"
  ;;
count)
  echo $($DIR/count)
  ;;
edit)
  $DIR/edit "${@:2}"
  ;;
close)
  $DIR/close "${@:2}"
  ;;
list)
  $DIR/list
  ;;
show)
  $DIR/show "${@:2}"
  ;;
init)
  $DIR/init "${@:2}"
  ;;
sync)
  $DIR/sync
  ;;
browser)
  $DIR/browser
  ;;
config)
  $DIR/editconfig
  ;;
*)
  echo "Unknown '$1' command."
  ;;
esac
